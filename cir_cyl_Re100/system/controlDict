FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     icoFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         250;

deltaT          0.01;

writeControl    timeStep;

writeInterval   50;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
	vorticity
	 {
	 	type		vorticity;
	 	libs		("libfieldFunctions.so");
	 	writeControl	writeTime;
	 }
	forces
	{
		type 		forceCoeffs;
		libs 		("libforces.so");
		log 		yes;
		writeControl 	timeStep;
		writeInterval 	20;
		
		patches
		(
				cylinder
		);
		rho 		rhoInf;
		p 		p;
		U 		U;
		rhoInf 	1;
		CofR 		(0 0 0);
		liftDir 	(0 1 0);
		dragDir 	(1 0 0);
		pitchAxis 	(0 0 1);
		magUInf 	1;
		lRef 		1;
		Aref 		1;
	}
}
// ************************************************************************* //
